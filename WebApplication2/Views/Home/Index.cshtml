@{
    ViewBag.Title = "Home Page";
}

<div class="row row-search">
    <div class="col-lg-12">
        <div class="input-group">
            <input id="input_text" type="text" class="form-control" placeholder="Search for...">
            <span class="input-group-btn">
                <button id="submit_button" class="btn btn-default" type="button">Get quotes</button>
            </span>
            
        </div>
    </div>
</div>


<div class="row">
    <div class="col-xs-3" >
        <div class="form-group">
            <div class="input-group date" id="datetimepicker1">
                <input type="text" class="form-control" id="input_datetimepicker1"/>
                <span class="input-group-addon">
                    <span class="glyphicon glyphicon-calendar"></span>
                </span>
            </div>
        </div>
    </div>
    <div class="col-xs-3">
        <div class="form-group">
            <div class="input-group date" id="datetimepicker2">
                <input type="text" class="form-control" id="input_datetimepicker2"/>
                <span class="input-group-addon">
                    <span class="glyphicon glyphicon-calendar"></span>
                </span>
            </div>
        </div>
    </div>
    <div class="col-xs-3">
        <select class="form-control" id="select_service">
            <option value="Google">Google</option>
            <option value="Yahoo">Yahoo</option>
        </select>
    </div>
</div>

<div id="messages"></div>


<div class="row">
    <div class="col-md-12">
        <div id="columns"></div>
    </div>
</div>


